{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "devCommand": "npm start",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
} 